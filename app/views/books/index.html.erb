
<%= render 'users/list', user: @user %>

<%= render 'books/list', book: @book %>

<% @books.each do |book| %>
  <h2>Books</h2>
  <table>
    <thead>
      <tr>
        <th></th>
        <th>Title</th>
        <th>Oponion</th>
        <th coispan="3"></th>
      </tr>
    </thead>
    <tbody>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td><%= link_to 'Show', book_path(book.id) %></td>
    </tbody>
  </table>
<% end %>

